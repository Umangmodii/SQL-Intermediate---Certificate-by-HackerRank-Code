
SET NOCOUNT ON;

WITH ROW_NUMBER AS (

SELECT
       USER_ACCOUNT_ID,
       UA.FIRST_NAME,
       UA.lAST_NAME,
       CUSTOMER_ID,
       CUSTOMER_NAME,
       ROW_NUMBER() OVER(PARTITION BY USER_ACCOUNT_ID,CUSTOMER_ID ORDER BY USER_ACCOUNT_ID,CUSTOMER_ID) AS ROW#
FROM CONTACT C
JOIN CUSTOMER CUST ON CUST.ID = C.CUSTOMER_ID
JOIN USER_ACCOUNT UA ON UA.ID = C.USER_ACCOUNT_ID

)

SELECT * FROM ROW_NUMBER WHERE ROW# = 2 ORDER BY USER_ACCOUNT_ID, CUSTOMER_ID
go
